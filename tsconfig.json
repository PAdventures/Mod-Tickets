{
  "include": ["./src/**/*"],
  "compilerOptions": {
    "moduleResolution": "NodeNext",
    "rootDir": "./src",
    "outDir": "./modules",
    "target": "ESNext",
    "module": "NodeNext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "resolveJsonModule": true
  }
}